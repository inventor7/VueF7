{
  "name": "vue-f7",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "Presalio 2.0 - A modern mobile-first application built with Vue 3, Framework7, and Capacitor for cross-platform deployment",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayb/presalio-2.0.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "build:ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build:android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "preview": "vite preview --port 4173",
    "ios": "vite build && npx cap copy ios",
    "ios-open": "vite build && npx cap copy ios && npx cap open ios",
    "android": "vite build && npx cap copy android",
    "android-open": "vite build && npx cap copy android && npx cap open android",
    "type-check": "vue-tsc --noEmit"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.2",
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@journeyapps/wa-sqlite": "^1.3.3",
    "@powersync/capacitor": "^0.1.3",
    "@powersync/web": "^1.29.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tailwindcss/vite": "^4.1.13",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dom7": "^4.0.6",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-vue": "^8.3.4",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.7.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@iconify-json/f7": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.22",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.0.0",
    "less": "^4.4.1",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^10.4.0",
    "typescript": "^5.9.2",
    "unplugin-imagemin": "^0.6.9",
    "unplugin-turbo-console": "^2.2.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.0.8"
  }
}
